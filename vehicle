class Vehicle:
    def move(self):
        print("The vehicle is moving...")

class Car(Vehicle):
    def move(self):
        print("ğŸš— The car is driving on the road.")

class Plane(Vehicle):
    def move(self):
        print("âœˆï¸ The plane is flying in the sky.")

class Boat(Vehicle):
    def move(self):
        print("â›µ The boat is sailing on the water.")

# Polymorphism in action
vehicles = [Car(), Plane(), Boat()]

for v in vehicles:
    v.move()
